# SyftHub API Configuration
# Copy this file to .env and fill in your values

# Required: URL of your SyftHub backend instance
SYFTHUB_URL=http://localhost:8080

# Required: Your SyftHub credentials
SYFTHUB_USERNAME=your-username
SYFTHUB_PASSWORD=your-password

# Required: The publicly accessible URL of your SyftAI Space
# This must match the host and port you run the space on
SPACE_URL=http://localhost:8001

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Optional: Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8001
